{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "no-unused-vars": "off",
    "object-curly-newline": "off",
    "prefer-template": "off",
    "no-return-await": "off",
    "class-methods-use-this": "off",
    "no-unneeded-ternary": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  }
}